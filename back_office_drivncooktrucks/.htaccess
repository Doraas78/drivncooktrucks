<IfModule mod_rewrite.c>
   Options +FollowSymLinks
   RewriteEngine on

   # Send request via index.php
   RewriteCond %{REQUEST_FILENAME} !-f
   RewriteCond %{REQUEST_FILENAME} !-d
   RewriteRule ^(.*)$ index.php/$1
   RewriteRule ^https://www.drivncooktrucks.com/([a-zA-Z]+)/([a-zA-Z]+)/([a-zA-Z]+)$ /index.php?p=$1&c=$2&a=$3 [L]
   Rewritecond %{HTTP_HOST} ^drivncooktrucks.com$
   Rewriterule ^(.*) http://www.drivncooktrucks.com/$1 [QSA,L,R=301]
</IfModule>